{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@svg/*": [
        "./src/assets/svg/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@types/*": [
        "./src/types/*"
      ]
    }
  }
}